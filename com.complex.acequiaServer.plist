<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>Label</key>
        <string>com.complex.acequiaServer</string>
      
        <key>ProgramArguments</key>
        <array>
          <string>/usr/bin/node</string>
          <string>$HOME/Documents/acequia/acequia/acequia.js</string>
          <string>127.0.0.1</string>
        </array>
        
        <key>StandardOutPath</key>
        <string>/tmp/acquialog.log</string>
  
        <key>StandardErrorPath</key>
        <string>/tmp/acequiaError.log</string>
        
        <key>KeepAlive</key>
        <true/>
        
        <key>RunAtLoad</key>
        <true/>

</dict>
</plist>